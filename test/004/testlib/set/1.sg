  S4 2 setf.remove = (| *S5, 2 value$ |),
  S5 setf.take >> case {
  ; (| 1, *S5' |) value$ ->
    S5' setf.take = (| 3, *S5'' |) value$,
    S5'' setf.take = none$
  ; (| 3, *S5' |) value$ ->
    S5' setf.take = (| 1, *S5'' |) value$,
    S5'' setf.take = none$
  },
  S5 setf.clear >> setf.empty? = true$,
  S5 [ 4, 5 ] setf.add_list = *S6,
  S6 setf.size = 4,
  S6 setf.iter = *I,
  I next = (| **, *I' |) value$,
  I' next = (| **, *I'' |) value$,
  I'' next = (| **, *I''' |) value$,
  I''' next = (| **, *I'''' |) value$,
