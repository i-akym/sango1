import "sango.debug" -> debug ;;
import "sango.io.stdio" -> stdio ;;
import "sango.util.ralist" -> ral ;;
import "sango.util.avltree.ralist" -> avlral ;;

extend <bool> -> my_bool := undef$ ;;

eval _main_ -> <void> {
  test
} ;;

eval <bool ral.ralist> *L f -> <void> {} ;;
eval <my_bool ral.ralist> *L g -> <void> {} ;;

eval test -> <void> {
  test000,
  test100,
  void$
} ;;
eval test000 -> <void> {
  avlral.new_ralist [ 1, 2, 3 ] ral.append_list
    >> ral.as_fxralist >> ral._feature_a'ralist = *L3,
  L3 _feature_a'col = *C3,
  C3 size = 3,
  C3 empty? = false$,
  C3 take = (| **, *C2 |) value$,
  C2 size = 2,
  C2 take = (| **, *C1 |) value$,
  C1 size = 1,
  C1 take = (| **, *C0 |) value$,
  C0 size = 0,
  C0 empty? = true$,
  C0 take = none$,
  void$
} ;;
eval test100 -> <void> {
  avlral.new_ralist = *L0,
  L0 true$ ral.append = *L1,  # <bool ral.ralist>
  L0 undef$ ral.append = *L2,  # <my_bool ral.ralist>
  L1 f,
  L1 g,
  L2 g,
  void$
} ;;
