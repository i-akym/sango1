import "sango.debug" -> debug ;;
import "sango.io.stdio" -> stdio ;;
import "sango.entity" -> ent ;;
import "sango.entity.existence" -> ex ;;

eval _main_ -> <void> {
  test,
  stdio.flush
} ;;

eval test -> <void> {
  test000,
  test100,
  void$
} ;;

eval test000 -> <void> {
  none$ none$ ex.create_immutable_existence = (| *E, ** |),
  E ent.undress = (| E, *RF |),
  E &RF = E,
  void$
} ;;
eval test100 -> <void> {
  none$ none$ ex.create_immutable_existence = (| *E, ** |),
  E ex.uniqueness >> debug.repr >> stdio.print_and_newline
} ;;
